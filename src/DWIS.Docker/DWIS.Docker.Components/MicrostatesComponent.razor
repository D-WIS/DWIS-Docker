@using MudBlazor;
@using DWIS.Docker.Constants;
@using DWIS.Docker.Clients;
@using DWIS.Docker.ModuleConfigurations;

<MudTabs Outlined="true" Position="Position.Left" Border="true">

    <MudTabPanel Text="Interpretation engine"  >

      
        <GenericModuleComponent TModuleConfig=MicroStateInterpretationConfiguration
                                ConfigFileName="@Names.MICROSTATE_INTERPRETATION_ENGINE_CONFIGFILENAME"
                                ContainerConfigPath="@Names.MICROSTATE_INTERPRETATION_ENGINE_CONTAINERPATH"
                                ImageName="@Names.MICROSTATES_INTERPRETATION_ENGINE_NOTAG"
                                ImageTag="@Names.STABLE_TAG"
                                LocalConfigPath="@Names.MICROSTATE_INTERPRETATION_ENGINE_LOCALPATH"
                                    _newContainerName=@Names.MICROSTATE_INTERPRETATION_ENGINE_CONTAINER_NAME />
      
    </MudTabPanel>

    <MudTabPanel Text="Threshold generator" >
        
        <GenericModuleComponent TModuleConfig=MicroStateThresholdsConfiguration
                                ConfigFileName="@Names.MICROSTATE_GENERATOR_CONFIGFILENAME"
                                ContainerConfigPath="@Names.MICROSTATE_GENERATOR_CONTAINERPATH"
                                ImageName="@Names.MICROSTATES_THRESHOLDS_NOTAG"
                                ImageTag="@Names.STABLE_TAG"
                                LocalConfigPath="@Names.MICROSTATE_GENERATOR_LOCALPATH"
                                      _newContainerName=@Names.MICROSTATE_THRESHOLDS_CONTAINER_NAME />
       
    </MudTabPanel>
</MudTabs>

